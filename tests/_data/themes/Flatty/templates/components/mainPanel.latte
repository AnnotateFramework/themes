<div id="main-panel">
    <div class="container">
        <a n:href="#dashboard" id="brand"><img src="{$themeDir}images/logo.png"> AnnotateCms</a>

        <div class="pull-right" id="right-panel">
            {foreach $links as $link}
                <a n:href="$link->url" data-toggle="tooltip"
                   title="{$link->title}"{ifCurrent $link->url} class="active"{/ifCurrent}>
                    <i class="{$link->icon}"></i>
                </a>
            {/foreach}
        </div>
    </div>
</div>