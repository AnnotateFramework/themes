{
  "name": "annotate/themes",
  "type": "annotate-extension",
  "description": "Themes support for Annotate Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Michal Vyšinský",
      "email": "vysinsky@live.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dotblue/codesniffer-ruleset.git"
    }
  ],
  "require": {
    "php": ">=5.4",
    "annotate/extensions-installer": "~2.0.0",
    "annotate/diagnostics": "~2.0.0",
    "annotate/templating": "~2.0.0",
    "annotate/events": "~1.0.0",
    "nette/bootstrap": "~2.2.0",
    "nette/http": "~2.2.0",
    "nette/security": "~2.2.0",
    "nette/mail": "~2.2.0",
    "nette/application": "~2.2.0",
    "latte/latte": "~2.2.0",
    "tracy/tracy": "~2.2.0",
    "nette/safe-stream": "~2.2.0",
    "nette/caching": "~2.2.0"
  },
  "require-dev": {
    "nette/tester": "@dev",
    "janmarek/mockista": "@dev",
    "squizlabs/php_codesniffer": "~2.1",
    "dotblue/codesniffer-ruleset": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Annotate\\Themes\\": "src/"
    }
  },
  "extra": {
    "extension-name": "themes",
    "extension-class": "Annotate\\Themes\\DI\\ThemesExtension"
  }
}
