{
    "name": "annotatecms/themes",
    "type": "annotatecms-extension",
    "description": "Themes support for AnnotateCms framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Vyšinský",
            "email": "vysinsky@live.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "annotatecms/extensions-installer": "@alpha",
        "annotatecms/diagnostics": "@alpha",
        "annotatecms/templating": "@alpha",
        "annotatecms/events": "@alpha",
        "nette/bootstrap": "~2.2.0"
    },
    "require-dev": {
        "nette/tester": "@dev",
        "janmarek/mockista": "@dev"
    },
    "suggest": {
        "annotatecms/packages": "Load themes dependencies automatically with packages package"
    },
    "autoload": {
        "psr-4": {
            "AnnotateCms\\Themes\\": "src/"
        }
    },
    "extra": {
        "extension-name": "themes",
        "extension-class": "AnnotateCms\\Themes\\DI\\ThemesExtension"
    }
}
