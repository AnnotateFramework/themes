{
  "name": "annotate/themes",
  "type": "library",
  "description": "Themes support for Annotate Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Michal Vyšinský",
      "email": "michal@vysinsky.cz"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:AnnotateFramework/coding-standard.git"
    }
  ],
  "require": {
    "php": ">=5.6",
    "annotate/diagnostics": "@dev",
    "annotate/templating": "@dev",
    "kdyby/events": "~3.0.0",
    "tracy/tracy": "~2.4.0",
    "nette/bootstrap": "~2.4.0",
    "nette/di": "~2.4.0",
    "nette/finder": "~2.4.0",
    "nette/application": "~2.4.0",
    "nette/robot-loader": "~2.4.0"
  },
  "require-dev": {
    "nette/tester": "@dev",
    "latte/latte": "~2.4.0",
    "squizlabs/php_codesniffer": "~2.3",
    "dotblue/codesniffer-ruleset": "@dev",
    "jakub-onderka/php-parallel-lint": "0.*",
    "jakub-onderka/php-console-highlighter": "0.*",
    "phpspec/prophecy": "~1.4",
    "codeclimate/php-test-reporter": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Annotate\\Themes\\": "src/"
    }
  }
}
